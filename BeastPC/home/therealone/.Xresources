!! Colorscheme

! special
URxvt.depth      : 32
!*.foreground    :   #f2f7c8
*.foreground     : #e7f096
URxvt.background : rgba:0000/0000/0000/eeee
xterm*background : #000000
URxvt.cursorColor:  #c5c8c6

! black
*.color0:  #1d1f21
*.color8:  #969896

! red
*.color1:  #cc342b
*.color9:  #cc342b

! green
*.color2:  #198844
*.color10: #198844

! yellow
*.color3:  #fba922
*.color11: #fba922

! blue
*.color4:  #3971ed
*.color12: #3971ed

! magenta
*.color5:  #a36ac7
*.color13: #a36ac7

! cyan
*.color6:  #3971ed
*.color14: #3971ed

! white
*.color7:  #c5c8c6
*.color15: #ffffff

!! URxvt Appearance
!URxvt.font          : xft:Inconsolata     :size=8
!URxvt.font          : xft:Mono            :pixelsize=14:antialias=true
URxvt.font           : xft:MesloLGS NF     :pixelsize=14
!URxvt*.font         : xft:MesloLGS NF     :pixelsize=14:antialias=true
!URxvt.boldFont      : xft:
!URxvt.italicFont    : xft:DejaVu Sans Mono:pixelsize=15,style=Book
!URxvt.boldItalicfont: xft:DejaVu Sans Mono:pixelsize=15,style=Book
URxvt.letterSpace    : 0
URxvt.lineSpace      : 0
URxvt.geometry       : 92x24
!URxvt.internalBorder: 24
URxvt.internalBorder : 8
URxvt.cursorBlink    : true
URxvt.cursorUnderline: false
URxvt.saveline       : 2048
URxvt.scrollBar      : false
URxvt.scrollBar_right: false
URxvt.urgentOnBell   : true
URxvt.iso14755       : false

!! URxvt Functions
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

URxvt.keysym.Control-Up   : \033[1;5A
URxvt.keysym.Control-Down : \033[1;5B
URxvt.keysym.Control-Left : \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C

URxvt.perl-ext-common      : default,tabbed,matcher,resize-font,-tabbed
URxvt.keysym.C-KP_Subtract : resize-font:smaller
URxvt.keysym.C-KP_Add      : resize-font:bigger
URxvt.keysym.C-equal       : resize-font:reset
URxvt.keysym.C-question    : resize-font:show
